<% provide(:title, 'All users') %>
<h1><%= t("global.canpaigs.allusers") %></h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Level</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <th scope="row"><%= user.id %></th>
      <td><%= user.name %></td>
      <td><%= user.level %></td>
    </tr>
    <% end %>
  </tbody>
</table>
	
<%= will_paginate %>


