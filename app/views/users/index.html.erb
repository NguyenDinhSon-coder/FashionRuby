
<% provide(:title, 'All users') %>
<%= will_paginate %>
<% if current_user.admin?  %>
    <%= link_to t("global.header.adminmanager"),new_admin_canpaign_path,class: 'btn btn-success'%>
    <%= link_to t("global.header.admin1"),admin_products_path,class: 'btn btn-success'%>
<% end %>
<%= will_paginate %>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col"><%= t("global.canpaigs.name") %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate %>


